    "# Trading Simulator or Strategy analyzer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Biases & Defect included: account transaction provisions,survival bias and peaking bias,\n",
    "# don't assume you can buy at the open or at a price near the close of the previous day;\n",
    "# the symbols.txt contains the 2019 S&P500 index tickers\n",
    "# to avoid survival bias Initiate a list of symbols that composed the S&P500 index in 2000–1–1.\n",
    "# For every update in the index (one symbol gets added, the other one is removed),\n",
    "# I should update my list of symbols as well.\n",
    "# the list of all changes to the S&P500 index for $97,this is a bit too much for me."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import datetime as dt\n",
    "import math\n",
    "from pandas_datareader import data as wb\n",
    "import warnings\n",
    "warnings.filterwarnings(\"ignore\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Expect 40 minutes wait to gather the all the data points\n",
    "#This works only if we have stored all symbols in a file called symbols.txt.\n",
    "stocks=[]\n",
    "f = open(\"symbols.txt\",\"r\")\n",
    "for line in f:\n",
    "    stocks.append(line.strip())\n",
    "f.close()\n",
    "\n",
    "wb.DataReader(stocks,\"yahoo\",start=\"2000-1-1\",end=\"2019-12-31\")[\"Adj Close\"].to_csv(\"prices.csv\")\n",
    "wb.DataReader(stocks,\"yahoo\",start=\"2000-1-1\",end=\"2019-12-31\")[\"Volume\"].to_csv(\"volume.csv\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "#initializing global variables\n",
    "# Prices = Pandas Dataframe where we obtain stock prices\n",
    "prices = pd.read_csv(\"prices.csv\", index_col=\"Date\", parse_dates=True)\n",
    "# valumechanges = originally, volume.csv stores daily trading volumes. \n",
    "#In our case, we are interested in percentage change. \n",
    "#The values are multiplied by 100 to make them more understandable: for example, 0.1 is the same as 10%;\n",
    "volumechanges = pd.read_csv(\"volume.csv\", index_col=\"Date\", parse_dates=True).pct_change()*100\n",
    "#today and simend = both of them are dates.\n",
    "#The first one defines the starting point of the whole simulation \n",
    "# and changes as the simulation iterates towards its end. The simend variable, \n",
    "#however, defines the end of the simulation;\n",
    "today = dt.date(2000, 1, 15)\n",
    "simend = dt.date(2019, 12, 31)\n",
    "#tickers = in this array we will store all the symbols we can trade\n",
    "tickers = []\n",
    "#transactionid = when opening/closing position, every transaction will have a unique id.\n",
    "#This is important if we want to put our transactions into an Excel file\n",
    "transactionid = 0\n",
    "#money = this is equivalent to cash at hand. In the beginning, we have 1,000,000$;\n",
    "money = 1000000\n",
    "#portfolio = this dictionary is for recording all the symbols and amounts we are currently \n",
    "# having in our portfolio. Keys are symbols and values are amounts (integer)\n",
    "portfolio = {}\n",
    "#activelog and transactionlog: these two arrays are for keeping track of our transactions. \n",
    "#The first one stores open positions only while the latter stores every transaction we make throughout 20 years.\n",
    "#I could’ve made them as one but the code runs faster if they are separated.\n",
    "activelog = []\n",
    "transactionlog = []"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Extracting the current stock price\n",
    "#id = a unique identifier for each transaction. Corresponding buy/sell transactions will have the same id;\n",
    "#ticker = stock symbol;\n",
    "#amount = the amount that was bought/sold;\n",
    "#price = the price at which stocks were bought/sold;\n",
    "#date = the date our transaction was made;\n",
    "#type = this can either be “buy” or “sell”;\n",
    "#exp_date = if we buy stock, we define exp_date as a date on which we want to close our position.\n",
    "#Currently, the difference between date and exp_date is 14 days.\n",
    "#info = any additional information we might want to add, we will leave it empty for our simulation.\n",
    "def getprice(date, ticker):\n",
    "    global prices\n",
    "    return prices.loc[date][ticker]\n",
    "\n",
    "\n",
    "def transaction(id, ticker, amount, price, type, info):\n",
    "    global transactionid\n",
    "    if type == \"buy\":\n",
    "        exp_date = today + dt.timedelta(days=14)\n",
    "        transactionid += 1\n",
    "    else:\n",
    "        exp_date = today\n",
    "    if type == \"sell\":\n",
    "        data = {\"id\": id, \"ticker\": ticker, \"amount\": amount, \"price\": price, \"date\": today, \"type\": type,\n",
    "                \"exp_date\": exp_date, \"info\": info}\n",
    "    elif type == \"buy\":\n",
    "        data = {\"id\": transactionid, \"ticker\": ticker, \"amount\": amount, \"price\": price, \"date\": today, \"type\": type,\n",
    "                \"exp_date\": exp_date, \"info\": info}\n",
    "        activelog.append(data)\n",
    "    transactionlog.append(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "#The buy() function takes two arguments as input: a list of symbols we are interested in buying\n",
    "#and the amount of money allocated for the purchase.\n",
    "def buy(interestlst, allocated_money):\n",
    "    global money, portfolio\n",
    "    for item in interestlst:\n",
    "        price = getprice(today, item)\n",
    "        if not np.isnan(price):\n",
    "            quantity = math.floor(allocated_money/price)\n",
    "            money -= quantity*price\n",
    "            portfolio[item] += quantity\n",
    "            transaction(0, item, quantity, price, \"buy\", \"\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "#This works pretty much in a similar way as the buy() function but will take positions from the activelog array and try to sell it.\n",
    "#It will try to sell only if the exp_date variable is smaller or equal to the today date.\n",
    "def sell():\n",
    "    global money, portfolio, prices, today\n",
    "    itemstoremove = []\n",
    "    for i in range(len(activelog)):\n",
    "        log = activelog[i]\n",
    "        if log[\"exp_date\"] <= today and log[\"type\"] == \"buy\":\n",
    "            tickprice = getprice(today, log[\"ticker\"])\n",
    "            if not np.isnan(tickprice):\n",
    "                money += log[\"amount\"]*tickprice\n",
    "                portfolio[log[\"ticker\"]] -= log[\"amount\"]\n",
    "                transaction(log[\"id\"], log[\"ticker\"], log[\"amount\"], tickprice, \"sell\", log[\"info\"])\n",
    "                itemstoremove.append(i)\n",
    "            else:\n",
    "                log[\"exp_date\"] += dt.timedelta(days=1)\n",
    "    itemstoremove.reverse()\n",
    "    for elem in itemstoremove:\n",
    "        activelog.remove(activelog[elem])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "#During a simulation iteration, we need to do the following:\n",
    "#Find the symbols that we think are worth buying.\n",
    "#Sell stocks that we must sell today (two week holding period is over).\n",
    "#Buy stocks that our strategy suggests.\n",
    "\n",
    "def simulation():\n",
    "    global today, volumechanges, money\n",
    "    start_date = today - dt.timedelta(days=14)\n",
    "    series = volumechanges.loc[start_date:today].mean()\n",
    "    interestlst = series[series > 100].index.tolist()\n",
    "    sell()\n",
    "    if len(interestlst) > 0:\n",
    "        #moneyToAllocate = 500000/len(interestlst)\n",
    "        moneyToAllocate = currentvalue()/(2*len(interestlst))\n",
    "        buy(interestlst, moneyToAllocate)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getindices():\n",
    "    global tickers\n",
    "    f = open(\"symbols.txt\", \"r\")\n",
    "    for line in f:\n",
    "        tickers.append(line.strip())\n",
    "    f.close()\n",
    "\n",
    "\n",
    "def tradingday():\n",
    "    global prices, today\n",
    "    return np.datetime64(today) in list(prices.index.values)"
   ]
  },
